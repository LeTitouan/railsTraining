<h4>New Post</h4>

<%= form_for @post do |f| %>

  <div class="form-group">
    <label>Title</label>
    <%= f.text_field :title, class: "form-control" %>
    <label>Content</label>
    <%= f.text_field :content, class: "form-control" %>
  </div>
  <div class="form-group">
    <button type="submit", class="btn btn-primary">comfirm changes</button>
  </div>
<%end%>


